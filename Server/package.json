{
  "name": "safestreets",
  "version": "1.0.0",
  "description": "SafeStreets server for software engineering 2 project 2019/2020",
  "main": "index.js",
  "scripts": {
    "prestart": "npm install",
    "start": "node index.js",
    "test": "cross-env NODE_ENV=test jest -i --force-exit --testTimeout=10000"
  },
  "keywords": [
    "swagger"
  ],
  "jest": {
    "preset": "jest-puppeteer",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov"
    ]
  },
  "license": "Unlicense",
  "private": true,
  "dependencies": {
    "body-parser": "^1.19.0",
    "connect": "^3.2.0",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.3.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "form-data": "latest",
    "js-yaml": "^3.3.0",
    "knex": "^0.20.4",
    "lodash": "latest",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.0",
    "oas-tools": "^2.1.7",
    "pg": "^7.14.0",
    "rimraf": "^3.0.0",
    "serve-static": "^1.14.1"
  },
  "devDependencies": {
    "cross-env": "^6.0.3",
    "jest": "^24.9.0",
    "jest-puppeteer": "^4.4.0",
    "puppeteer": "^2.0.0",
    "supertest": "^4.0.2"
  }
}
